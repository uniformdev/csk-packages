{
  "name": "@uniformdev/csk-cli",
  "version": "6.0.82",
  "description": "Command-line interface (CLI) tool designed to streamline the development workflow within Uniform projects. It provides commands for pulling additional data and generating components based on Canvas data",
  "license": "SEE LICENSE IN LICENSE.txt",
  "engines": {
    "node": "^18.18.0 || ^19.8.0 || >= 20.0.0"
  },
  "main": "./cli.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./cli.js"
    },
    "./tailwindcss-conf": {
      "types": "./dist/tailwindcss-conf.d.mts",
      "default": "./dist/tailwindcss-conf.mjs"
    },
    "./components": {
      "types": "./dist/components.d.mts",
      "default": "./dist/components.mjs"
    }
  },
  "types": "./dist/index.d.mts",
  "scripts": {
    "csk-cli": "node ./cli.js",
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "test": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "eslint . --fix --rule \"prettier/prettier: error\""
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@inquirer/prompts": "^7.1.0",
    "commander": "^9.0.0",
    "dotenv": "16.4.7",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/jest": "^29.5.14",
    "@uniformdev/canvas": "^20.20.0",
    "next": "^15.3.2",
    "prettier": "3.5.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@uniformdev/canvas": ">=20",
    "next": ">=15",
    "prettier": ">=3",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "bin": {
    "csk-cli": "cli.js"
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
