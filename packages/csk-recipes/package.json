{
  "name": "@uniformdev/csk-recipes",
  "version": "6.0.110",
  "description": "command-line interface (CLI) and utility functions to help you work with recipes in a CSK project. It simplifies project initialization by allowing you to choose templates and include specific recipes",
  "license": "SEE LICENSE IN LICENSE.txt",
  "engines": {
    "node": "^18.18.0 || ^19.8.0 || >= 20.0.0"
  },
  "main": "./cli.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./cli.js"
    },
    "./tailwindcss-conf": {
      "types": "./dist/tailwindcss-conf.d.mts",
      "default": "./dist/tailwindcss-conf.mjs"
    },
    "./components": {
      "types": "./dist/components.d.mts",
      "default": "./dist/components.mjs"
    }
  },
  "types": "./dist/index.d.mts",
  "scripts": {
    "csk-recipes": "node ./cli.js",
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "eslint . --fix --rule \"prettier/prettier: error\""
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@inquirer/prompts": "^7.1.0",
    "@uniformdev/metascript": "^1.0.0",
    "commander": "^9.0.0",
    "dotenv": "16.4.7",
    "open": "^10.1.2",
    "ora": "^8.1.1",
    "prettier": "3.5.2"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/jest": "^29.5.14",
    "@uniformdev/canvas": "^20.23.0",
    "next": "^15.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@uniformdev/canvas": ">=20",
    "next": ">=15",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "bin": {
    "csk-recipes": "cli.js"
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
