{
  "name": "csk-v-next",
  "version": "1.0.57",
  "private": true,
  "engines": {
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm",
    "node": ">=18",
    "npm": ">=10"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "workspace:check": "manypkg check",
    "workspace:fix": "manypkg fix",
    "prepare": "husky"
  },
  "packageManager": "npm@10.7.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "lint-staged": {
    "*.{ts,tsx}": "eslint"
  },
  "dependencies": {
    "@manypkg/cli": "^0.21.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "tsx": "4.16.2",
    "turbo": "^2.0.6"
  }
}
