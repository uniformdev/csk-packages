{
  "name": "csk-v-next",
  "version": "0.1.179",
  "private": true,
  "engines": {
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm",
    "node": ">=18",
    "npm": ">=10"
  },
  "scripts": {
    "dev:watch": "WATCH=true run-s uniform:pull:tokens dev",
    "dev": "npm run uniform:pull:locales && next dev",
    "build": "run-s uniform:pull:tokens uniform:pull:locales next:build",
    "next:build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky",
    "init": "run-s uniform:push:tokens uniform:push:content uniform:publish:manifest",
    "uniform:pull:tokens": "theme-pack pull",
    "uniform:push:tokens": "theme-pack push",
    "uniform:pull:content": "uniform sync pull",
    "uniform:push:content": "uniform sync push",
    "uniform:publish:manifest": "uniform context manifest publish",
    "uniform:pull:locales": "tsx src/scripts/get-locales.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "scaffold:add": "scaffold add",
    "modules:init": "csk-cli init"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint"
  },
  "dependencies": {
    "@uniformdev/canvas-next-rsc": "19.209.1",
    "@uniformdev/csk-cli": "*",
    "@uniformdev/scaffold": "*",
    "@uniformdev/theme-pack": "*",
    "classnames": "^2.5.1",
    "cookies-next": "^4.3.0",
    "next": "14.2.18",
    "next-themes": "^0.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-player": "^2.16.0",
    "react-responsive-masonry": "2.4.1",
    "tailwind-merge": "^2.5.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.8.0",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/nextjs": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@tailwindcss/typography": "0.5.14",
    "@types/node": "22.3.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/react-responsive-masonry": "2.1.3",
    "@typescript-eslint/eslint-plugin": "8.1.0",
    "@typescript-eslint/parser": "8.1.0",
    "@typescript-eslint/types": "8.1.0",
    "@uniformdev/cli": "19.209.1",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tailwindcss": "3.17.4",
    "husky": "9.1.4",
    "lint-staged": "15.2.9",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.41",
    "prettier": "3.3.3",
    "storybook": "^8.2.9",
    "tailwindcss": "3.4.10",
    "tsx": "4.17.0",
    "typescript": "5.5.4"
  }
}