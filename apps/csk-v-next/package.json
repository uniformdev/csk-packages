{
  "name": "csk-v-next",
  "version": "6.0.32",
  "private": true,
  "engines": {
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm",
    "node": "^18.18.0 || ^19.8.0 || >= 20.0.0",
    "npm": ">=10"
  },
  "scripts": {
    "dev": "run-s pull:dex pull:locales next:dev",
    "build": "run-s pull:locales pull:dex next:build",
    "start": "next start",
    "dev:watch": "cross-env WATCH=true run-s pull:dex next:dev",
    "next:dev": "next dev",
    "next:build": "next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "init": "run-s uniform:push uniform:publish",
    "component:extract": "csk-components extract",
    "component:scaffold": "csk-cli scaffold",
    "pull:dex": "design-extensions-tools pull",
    "push:dex": "design-extensions-tools push",
    "pull:locales": "csk-cli pull -l",
    "pull:content": "uniform sync pull",
    "push:content": "uniform sync push",
    "pull:content:dev": "uniform sync pull --config ./uniform.config.dev.ts",
    "push:content:dev": "uniform sync push --config ./uniform.config.dev.ts",
    "uniform:pull": "run-s pull:dex pull:content",
    "uniform:push": "run-s push:dex push:content",
    "uniform:publish": "uniform context manifest publish",
    "scaffold:add": "csk-cli scaffold",
    "recipes": "csk-recipes init"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.2.5",
    "@ai-sdk/react": "^1.1.23",
    "@next/third-parties": "^15.2.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@supabase/supabase-js": "^2.49.3",
    "@uniformdev/canvas-next-rsc": "^20.11.0",
    "@uniformdev/context-gtag": "^20.11.0",
    "@uniformdev/csk-components": "*",
    "@uniformdev/design-extensions-tools": "*",
    "ai": "^4.1.61",
    "next": "^15.2.3",
    "next-intl": "^3.26.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "ua-parser-js": "^2.0.3",
    "use-debounce": "^10.0.4",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@uniformdev/cli": "^20.11.0",
    "@uniformdev/csk-cli": "*",
    "@uniformdev/csk-recipes": "*",
    "cross-env": "^7.0.3",
    "eslint": "^9.19.0",
    "eslint-config-next": "^15.2.3",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.1",
    "prettier": "3.5.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  }
}
