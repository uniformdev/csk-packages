{
  "name": "csk-v-next",
  "version": "2.0.20",
  "private": true,
  "engines": {
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm",
    "node": ">=20",
    "npm": ">=10"
  },
  "scripts": {
    "dev:watch": "WATCH=true run-s pull:configuration next:dev",
    "dev": "run-s pull:locales next:dev",
    "next:dev": "next dev",
    "build": "run-s pull:configuration next:build",
    "next:build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "extract:components": "csk-components extract",
    "init": "run-s uniform:push uniform:publish",
    "uniform:push": "run-s push:configuration push:data",
    "pull:configuration": "design-extensions-tools pull",
    "push:configuration": "design-extensions-tools push",
    "pull:locales": "csk-cli pull -l",
    "pull:dev": "uniform sync pull --config ./uniform.config.dev.ts",
    "push:dev": "uniform sync push --config ./uniform.config.dev.ts",
    "pull:data": "uniform sync pull",
    "push:data": "uniform sync push",
    "uniform:publish": "uniform context manifest publish",
    "scaffold:add": "csk-cli scaffold",
    "recipes:init": "csk-recipes init"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.2",
    "@uniformdev/canvas-next-rsc": "20.1.0",
    "@uniformdev/context-gtag": "20.1.0",
    "@uniformdev/csk-components": "*",
    "@uniformdev/design-extensions-tools": "*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.474.0",
    "next": "15.1.6",
    "next-intl": "^3.26.3",
    "react": "19.0.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "19.0.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@uniformdev/cli": "20.1.0",
    "@uniformdev/csk-cli": "*",
    "@uniformdev/csk-recipes": "*",
    "eslint": "^9.19.0",
    "eslint-config-next": "^15.1.6",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  }
}
