{
  "name": "csk-v-next",
  "version": "1.0.34",
  "private": true,
  "engines": {
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm",
    "node": ">=18",
    "npm": ">=10"
  },
  "scripts": {
    "dev:watch": "run-s uniform:pull:tokens dev",
    "dev": "npm run uniform:pull:locales && next dev",
    "build": "run-s uniform:pull:tokens uniform:pull:locales next:build",
    "next:build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky",
    "init": "run-s uniform:push:tokens uniform:push:content uniform:publish:manifest",
    "uniform:pull:colors": "theme-pack pull -c",
    "uniform:push:colors": "theme-pack push -c",
    "uniform:pull:dimensions": "theme-pack pull -d",
    "uniform:push:dimensions": "theme-pack push -d",
    "uniform:pull:fonts": "theme-pack pull -f",
    "uniform:push:fonts": "theme-pack push -f",
    "uniform:pull:borders": "theme-pack pull -b",
    "uniform:push:borders": "theme-pack push -b",
    "uniform:pull:tokens": "theme-pack pull",
    "uniform:push:tokens": "theme-pack push",
    "uniform:pull:content": "uniform sync pull",
    "uniform:push:content": "uniform sync push",
    "uniform:publish:manifest": "uniform context manifest publish",
    "uniform:pull:locales": "tsx src/scripts/get-locales.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint"
  },
  "dependencies": {
    "@uniformdev/canvas-next-rsc": "19.206.0",
    "@uniformdev/theme-pack": "*",
    "classnames": "^2.5.1",
    "cookies-next": "^4.3.0",
    "next": "14.2.10",
    "next-themes": "^0.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-player": "^2.16.0",
    "react-responsive-masonry": "2.4.1",
    "tailwind-merge": "^2.5.2"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@tailwindcss/typography": "0.5.14",
    "@types/node": "22.3.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/react-responsive-masonry": "2.1.3",
    "@uniformdev/cli": "19.206.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.41",
    "tailwindcss": "3.4.10"
  }
}
