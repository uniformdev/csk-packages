{
  "name": "@uniformdev/component-starter-kit",
  "version": "6.1.60",
  "private": true,
  "engines": {
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm",
    "node": "^18.18.0 || ^19.8.0 || >= 20.0.0",
    "npm": ">=10"
  },
  "scripts": {
    "dev": "run-s pull:dex apply:dex pull:locales next:dev",
    "build": "run-s pull:locales pull:dex apply:dex next:build",
    "start": "next start",
    "dev:watch": "cross-env WATCH=true run-s pull:dex apply:dex next:dev",
    "next:dev": "next dev",
    "next:build": "next build",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "init": "csk-cli init --dev",
    "component:extract": "csk-components extract",
    "component:scaffold": "csk-cli scaffold",
    "pull:dex": "design-extensions-tools pull",
    "apply:dex": "design-extensions-tools apply",
    "push:dex": "design-extensions-tools push",
    "pull:locales": "csk-cli pull -l",
    "pull:content": "uniform sync pull",
    "push:content": "uniform sync push",
    "pull:content:dev": "uniform sync pull --config ./uniform.config.dev.ts",
    "push:content:dev": "uniform sync push --config ./uniform.config.dev.ts",
    "uniform:pull": "csk-cli pull -u --dev",
    "uniform:push": "run-s push:dex push:content",
    "uniform:publish": "uniform context manifest publish",
    "recipes": "csk-recipes init"
  },
  "dependencies": {
    "@uniformdev/assets": "20.7.1-alpha.120",
    "@uniformdev/canvas": "20.7.1-alpha.120",
    "@uniformdev/csk-components": "*",
    "@uniformdev/design-extensions-tools": "*",
    "@uniformdev/next-app-router": "20.7.1-alpha.120",
    "@uniformdev/next-app-router-client": "20.7.1-alpha.120",
    "@uniformdev/next-app-router-shared": "20.7.1-alpha.120",
    "next": "^16.0.10",
    "next-themes": "^0.4.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@uniformdev/cli": "20.7.1-alpha.120",
    "@uniformdev/csk-cli": "*",
    "@uniformdev/csk-recipes": "*",
    "cross-env": "^10.1.0",
    "eslint": "^9.31.0",
    "eslint-config-next": "^16.0.10",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.7.3"
  }
}
