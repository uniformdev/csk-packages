{
  "name": "@uniformdev/component-starter-kit",
  "version": "6.1.34",
  "private": true,
  "engines": {
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm",
    "node": "^18.18.0 || ^19.8.0 || >= 20.0.0",
    "npm": ">=10"
  },
  "scripts": {
    "dev": "run-s pull:dex apply:dex pull:locales next:dev",
    "build": "run-s pull:locales pull:dex apply:dex next:build",
    "start": "next start",
    "dev:watch": "cross-env WATCH=true run-s pull:dex apply:dex next:dev",
    "next:dev": "next dev",
    "next:build": "next build",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "init": "run-s uniform:push uniform:publish",
    "component:extract": "csk-components extract",
    "component:scaffold": "csk-cli scaffold",
    "pull:dex": "design-extensions-tools pull",
    "apply:dex": "design-extensions-tools apply",
    "push:dex": "design-extensions-tools push",
    "pull:locales": "csk-cli pull -l",
    "pull:content": "uniform sync pull",
    "push:content": "uniform sync push",
    "pull:content:dev": "uniform sync pull --config ./uniform.config.dev.ts",
    "push:content:dev": "uniform sync push --config ./uniform.config.dev.ts",
    "uniform:pull": "run-s pull:dex apply:dex pull:content",
    "uniform:push": "run-s push:dex push:content",
    "uniform:publish": "uniform context manifest publish",
    "recipes": "csk-recipes init",
    "db:migrate": "tsx src/modules/chat/rag/db/migrate.ts",
    "db:drop": "drizzle-kit drop",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:load": "tsx src/modules/chat/rag/scripts/loadResources.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.2.5",
    "@ai-sdk/react": "^1.1.23",
    "@next/third-parties": "^15.3.2",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.1.1",
    "@uniformdev/canvas-next-rsc-client-v2": "20.7.1-alpha.81",
    "@uniformdev/canvas-next-rsc-shared-v2": "20.7.1-alpha.81",
    "@uniformdev/canvas-next-rsc-v2": "20.7.1-alpha.81",
    "@uniformdev/context-gtag": "20.7.1-alpha.81",
    "@uniformdev/csk-components": "*",
    "@uniformdev/design-extensions-tools": "*",
    "ai": "^4.1.61",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.7.1",
    "lucide-react": "^0.525.0",
    "nanoid": "^5.1.5",
    "next": "^15.6.0-canary.16",
    "next-intl": "^3.26.3",
    "postgres": "^3.4.5",
    "react": "^19.1.0",
    "react-day-picker": "^9.6.7",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "space-separated-tokens": "^2.0.2",
    "tailwind-merge": "^3.3.1",
    "ua-parser-js": "^2.0.4",
    "use-debounce": "^10.0.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.0.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@uniformdev/cli": "20.7.1-alpha.81",
    "@uniformdev/csk-cli": "*",
    "@uniformdev/csk-recipes": "*",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.0",
    "eslint": "^9.31.0",
    "eslint-config-next": "^15.6.0-canary.16",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "3.5.2",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.7.3"
  }
}
